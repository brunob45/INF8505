<prxusersettings>
    <zlayout>
        <perspective type="dev">
            <VerPaneNode>
                <HorPaneNode size="2">
                    <ViewBookNode size="2">
                        <window name="::ProjectView" visible="1" select="1"/>
                        <window name="::SrcRefView" visible="0" select="0"/>
                    </ViewBookNode>
                    <EditorBookNode size="3"/>
                </HorPaneNode>
                <ViewBookNode size="1">
                    <window name="::ConsoleView" visible="1" select="1"/>
                </ViewBookNode>
            </VerPaneNode>
        </perspective>
        <perspective type="iss">
            <HorPaneNode>
                <VerPaneNode size="3">
                    <ViewBookNode size="492">
                        <window name="::ProjectView" visible="0" select="0"/>
                        <window name="::MicView" visible="1" select="1"/>
                    </ViewBookNode>
                    <ViewBookNode size="401">
                        <window name="::StatsView" visible="1" select="0"/>
                        <window name="::RegisterView" visible="1" select="0"/>
                        <window name="::StorageView" visible="0" select="0"/>
                        <window name="::PipelineView" visible="1" select="0"/>
                        <window name="::IssWaveformView" visible="1" select="1"/>
                    </ViewBookNode>
                </VerPaneNode>
                <VerPaneNode size="4">
                    <EditorBookNode size="4">
                        <window name="::editor0" visible="1" select="1"/>
                    </EditorBookNode>
                    <ViewBookNode size="2">
                        <window name="::HostedIOView" visible="1" select="1"/>
                        <window name="::FileIOView" visible="0" select="0"/>
                        <window name="::VarInfoView" visible="0" select="0"/>
                        <window name="::LocalVarView" visible="1" select="0"/>
                        <window name="::BreakWatchView" visible="0" select="0"/>
                        <window name="::ProfilingView" visible="0" select="0"/>
                    </ViewBookNode>
                    <ViewBookNode size="1">
                        <window name="::ConsoleView" visible="1" select="1"/>
                    </ViewBookNode>
                </VerPaneNode>
            </HorPaneNode>
        </perspective>
        <editor id="::editor0" file="test1.s" top="1.0" cursor="1.0" foldmap="1" linemap="0"/>
    </zlayout>
    <active cfg="Release" dbg="&lt;None&gt;"/>
    <selectsim type="ISSCA" exe="&lt;PROCDIR&gt;/../iss/tinycore2_ca"/>
    <issui>iss varinfo::locals last_sort {1 -increasing}
iss profiling storage_access_history_logging Off
iss registers isvisible {controller Ports Registers} notvisible {}
iss storages isvisible {} notvisible {}
iss varinfo::statics last_sort {1 -increasing}
iss memory::DM start_field 0 vector 0
iss memory::PM start_field 0 vector 0
breakpoint mic add 15 -hitcount 1 -verbose 1 -exported 0 -software -1 -command {} -remove_when_hit 0
iss profiling value(stepped) 1 value(profile_storages) {DM PM}
iss waveform vars {time PC aluA aluB aluC jump_pc_offs_sig pc_offs} signal_info {time {color green num_format %x} PC {color green num_format %x} aluA {color green num_format %x} aluB {color green num_format %x} aluC {color green num_format %x} jump_pc_offs_sig {color green num_format %x} pc_offs {color green num_format %x}}</issui>
</prxusersettings>
